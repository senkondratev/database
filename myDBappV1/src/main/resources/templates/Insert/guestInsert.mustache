<!DOCTYPE html>
<html>
<head>
    <title>please work</title>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
</head>
<body>
<script src="https://code.jquery.com/jquery-2.2.4.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44=" crossorigin="anonymous"></script>
<script type="text/javascript">$(document).on("change", "#reservationSelect", function () {
    var selected = $(this).val();
    $.get("/insert/guestInsertSupportController",
            {
                selectedReservationId : selected
            }, function(html){
                jQuery('#roomSelect').html(html);
            });
})
</script>
    <div>
        <form action="/insert/guest" method="post">
            <select name="reservationId" id="reservationSelect">
                <option disabled selected>Choose reservation</option>
                {{#reservations}}
                    <option value={{reservationId}}>{{reservationId}} idReservation</option>
                {{/reservations}}
            </select>
            <select name="roomId" id="roomSelect">
                {{#rooms}}
                    <option value={{roomId}}>{{roomId}} idRoom</option>
                {{/rooms}}
            </select>
            <select name="companyId">
                {{#companies}}
                    <option value={{companyId}}>{{companyId}} idCompany</option>
                {{/companies}}
            </select>
            <select name="clientId">
                {{#clients}}
                    <option value={{clientId}}>{{clientId}} idClient</option>
                {{/clients}}
            </select>

            <select name="reviewType">
                <option value="false">false</option>
                <option value="true">true</option>
            </select>
            <input name="review" type="text">
            <button type="submit">ok</button>
        </form>
        {{status}}
    </div>
    <div>
        These are out guests
    </div>
    {{#guests}}
        {{guestId}}
        {{reservationId}} id reservation
        {{companyName}} company name
        {{clientSurname}} client surname
        {{roomId}} roomId
        {{reviewType}}
        {{guestReview}}
        <br>
    {{/guests}}
</body>
</html>